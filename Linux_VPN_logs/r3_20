subst_install_header() { file=`echo "$1" | sed -e 's,^.*/,,'`; /usr/bin/sed -e "s/\<EXPORT\>/extern/" < "$1" > "include/$file.t" || exit 1; /usr/bin/mkdir -p "$2" || exit 1; /usr/bin/install -c -m 644 "include/$file.t" "$2/$file" || exit 1; rm -f "include/$file.t"; }; subst_install_header "include/attributes.h" "/usr/include/attr"
